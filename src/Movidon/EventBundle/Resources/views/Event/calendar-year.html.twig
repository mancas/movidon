<div class="row">
    <ul class="list-unstyled navigation-events-calendar">
        <li><a id="prev-year" href="{{ path('events', {'year' : year-1}) }}" title="{{ 'Año anterior' | trans }}" class="fa fa-chevron-left"></a></li>
        <li class="year">{{ year }}</li>
        <li><a id="next-year" href="{{ path('events', {'year' : year+1}) }}" title="{{ 'Año próximo' | trans }}" class="fa fa-chevron-right"></a></li>
    </ul>
</div>
<div class="row events">
    {% for i in range(1, 12) %}
        <div class="col-xs-12 col-sm-6 col-md-4 col-lg-4">
            {{ showCalendarEvents(i ~ "/"~ year, events) }}
        </div>
    {% endfor %}
</div>